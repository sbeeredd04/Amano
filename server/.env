# Database Configuration (SQLite Example)
DATABASE_URL=sqlite:///songs.db

# AWS (Amazon Bedrock) Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-west-2  # The AWS region you're using for Bedrock services

# Spotify API Configuration
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# Flask App Configuration
FLASK_ENV=development
FLASK_APP=app.py

# Flask Secret Key (For session management if needed)
SECRET_KEY=your_flask_secret_key
